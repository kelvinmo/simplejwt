{
    "name": "kelvinmo/simplejwt",
    "description": "An simple JSON Web Token library for PHP.",
    "keywords": [ "jwt", "jwe", "jose" ],
    "homepage": "https://github.com/kelvinmo/simplejwt",
    "require": {
        "php": "^5.4 || ^7.1",
        "ext-openssl": "*",
        "ext-hash": "*",
        "ext-gmp": "*",
        "symfony/console": "~2.8 || ^4.0"
    },
    "require-dev": {
        "ext-bz2": "*",
        "ext-Phar": "*",
        "phpunit/phpunit": "4.*",
        "phing/phing": "2.*"
    },
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Kelvin Mo",
            "email": "kmo@users.sourceforge.net"
        }
    ],
    "bin": [ "bin/jwkstool" ],
    "autoload": {
        "psr-4": { "SimpleJWT\\": "src/SimpleJWT" }
    },
    "scripts": {
        "phar": [ "phing phar" ]
    }
}
